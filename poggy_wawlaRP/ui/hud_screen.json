{

 "anim_actionbar_text_alpha_out": {
  "anim_type": "alpha",
  "easing": "in_expo",
  "duration": 0.14,
  "from": 1.0,
  "to": 0,
  "destroy_at_end": "hud_actionbar_text"
},
"anim_actionbar_text_background_alpha_out": {
  "anim_type": "alpha",
  "easing": "in_expo",
  "duration": 0.14,
  "from": 1,
  "to": 0,
  "destroy_at_end": "hud_actionbar_text"
},
"auto_save_animation": {
  "anim_type": "flip_book",
  "initial_uv": [
    0,
    0
  ],
  "frame_count": 9,
  "frame_step": 18,
  "fps": 10,
  "reversible": true,
  "easing": "linear"
},
"auto_save": {
  "type": "image",
  "layer": 31,
  "texture": "textures/ui/auto_save",
  "anchor_from": "top_right",
  "anchor_to": "top_right",
  "offset": [
    -15,
    15
  ],
  "size": [
    18,
    30
  ],
  "uv_size": [
    18,
    30
  ],
  "uv": "@auto_save_animation",
  "disable_anim_fast_forward": true,
  "bindings": [
    {
      "binding_name": "#auto_save_animation_visible",
      "binding_name_override": "#visible"
    }
  ]
},

 "hud_actionbar_text": {
    "type": "image",
    "size": [
      "100%c + 12px",
      "100%c + 5px"
    ],
    "anchor_from": "top_middle",
    "offset": [
      0,
      "41px"
    ],
    "texture": "textures/ui/wawlaBorder",
    "alpha": 1,
    "controls": [
      {
        "actionbar_message": {
          "type": "label",
          "shadow": true,
          "anchor_from": "center",
          "anchor_to": "center",
          "color": "$tool_tip_text",
          "layer": 31,
          "text": "$actionbar_text",
          "localize": false,
          "alpha": "@hud.anim_actionbar_text_alpha_out"
        }
      }
    ]
  }

}